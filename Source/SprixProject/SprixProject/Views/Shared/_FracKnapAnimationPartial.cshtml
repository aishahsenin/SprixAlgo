@model SprixProject.ViewModel.AnimationViewModel
<div class="col-lg-12">
    <div class="col-lg-2"></div>
    <div class="col-lg-8">
        <div id="inHere">
            @for (int i = 0; i < Model.knapsackAnimationData.Items.Count(); i++)
            {
                var indexVal = i + 1;
                <div class="index" id=@("index" + indexVal)> Item @indexVal <br />Weight: @Model.knapsackAnimationData.Items[i].Weight <br />Value: @Model.knapsackAnimationData.Items[i].Benefit</div>
            }
            <p>Sack &nbsp;&nbsp;&nbsp; Capacity: @Model.knapsackAnimationData.Capacity</p>
            <div name="capacity" id="sack"></div>
        </div>
    </div>
    <div class="col-lg-2"></div>
</div>

<p id="testing"></p>

<div class="col-lg-12">
    <div class="col-lg-4"></div>
    <div class="col-lg-4">
        Test this
    </div>
    <div class="col-lg-4"></div>
</div>

<input type="button" id="testExecute" value="Test me" class="btn" onClick="startFractionalKnapsack()" />



